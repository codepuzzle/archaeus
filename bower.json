{
  "name": "archaeus",
  "version": "0.0.0",
  "dependencies": {
    "commonjs-require-definition": "*",
    "zepto": "*",
    "underscore": "*",
    "color": "http://github.com/harthur/color.git",
    "color-convert": "http://github.com/harthur/color-convert.git",
    "color-string": "http://github.com/harthur/color-string.git"
  },
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "overrides": {
    "underscore": {
      "main": "underscore.js"
    },
    "color": {
      "main": "color.js"
    },
    "color-convert": {
      "main": ["index.js", "conversions.js"]
    },
    "color-string": {
      "main": "color-string.js"
    }
  }
}
