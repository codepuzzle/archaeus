  "version": "0.0.0",
  "dependencies": {
    "zepto": "*",
    "underscore": "*",
    "socket.io-client": "~0.9.16"
  },
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "overrides": {
    "underscore": {
      "main": "underscore.js"
    },
    "color": {
      "main": "color.js"
    },
